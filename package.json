{
  "name": "@elonehoo/is-object",
  "description": "Checks whether a value is an object, because typeof is a troll.",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Elone Hoo <huchengyea@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/is-object"
  },
  "homepage": "https://github.com/elonehoo/is-object#readme",
  "bugs": "https://github.com/elonehoo/is-object/issues",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "18.16.14",
    "@vitest/coverage-c8": "^0.31.1",
    "bumpp": "9.1.0",
    "c8": "^7.13.0",
    "eslint": "8.41.0",
    "pnpm": "8.5.1",
    "typescript": "5.0.4",
    "unbuild": "^1.2.1",
    "vite": "4.3.8",
    "vitest": "0.31.1"
  }
}
