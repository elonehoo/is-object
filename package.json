{
  "name": "@elonehoo/is-object",
  "version": "0.0.5",
  "description": "Checks whether a value is an object, because typeof is a troll.",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/is-object#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/is-object"
  },
  "bugs": "https://github.com/elonehoo/is-object/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.25.1",
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "20.14.15",
    "@vitest/coverage-v8": "^1.6.0",
    "bumpp": "9.4.2",
    "c8": "^9.1.0",
    "eslint": "8.57.0",
    "pnpm": "8.15.9",
    "typescript": "5.5.4",
    "unbuild": "^2.0.0",
    "vite": "5.4.0",
    "vitest": "1.6.0"
  }
}
