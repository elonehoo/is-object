{
  "name": "@elonehoo/is-object",
  "version": "0.0.6",
  "description": "Checks whether a value is an object, because typeof is a troll.",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/is-object#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/is-object"
  },
  "bugs": "https://github.com/elonehoo/is-object/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.11.2",
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "22.10.1",
    "@vitest/coverage-v8": "^2.1.8",
    "bumpp": "9.9.0",
    "c8": "^10.1.2",
    "eslint": "9.16.0",
    "pnpm": "9.15.0",
    "typescript": "5.7.2",
    "unbuild": "^2.0.0",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  }
}
